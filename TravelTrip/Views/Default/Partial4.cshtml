@using TravelTrip.Models.Classes
@model List<Blog>

<div class="padding1">
    <h4>Some of Best Places</h4>
    <div class="box-left">
        @foreach (var x in Model)
        {


        <div class="bot-img">
            <img src="@x.BlogImage"  style="width:300px; height:120px;" alt="" />
        </div>
        <div class="bot-desc">
            <p>@x.Explanation.Substring(0, 100)</p>
            <br />
            <p><a href="/Blog/BlogDetail/@x.ID"><span class="read">Read More >></span></a></p>
        </div>
        <div class="clear"></div>
        
        }
    </div>
    @Html.Action("Partial5", "Default");
</div>